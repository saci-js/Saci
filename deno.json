{
  "name": "@saci-js/saci",
  "version": "1.0.0",
  "exports": "./src/mod.ts",
  "license": "MIT",
  "tasks": {
    "test": "deno test tests/*",
    "ok": "deno lint && deno fmt",
    "test:node": "npm i && node _tools/run_test.mjs",
    "test:bun": "npm i && bun test _tools/run_test.mjs"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.14"
  },
  "lint": {
    "exclude": ["./examples", "_tools"],
    "rules": {
      "tags": ["recommended"],
      "include": [
        "camelcase",
        "no-console"
      ]
    }
  }
}
